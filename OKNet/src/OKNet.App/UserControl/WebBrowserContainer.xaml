<UserControl x:Class="OKNet.App.UserControl.WebBrowserContainer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OKNet.App.UserControl"
             xmlns:app="clr-namespace:OKNet.App"
             xmlns:viewModel="clr-namespace:OKNet.App.ViewModel"
             xmlns:design="clr-namespace:OKNet.App.ViewModel.Design"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" d:DataContext="{d:DesignInstance design:DesignWebBrowserViewModel, IsDesignTimeCreatable=True}">
    <WebBrowser app:WebBrowserUtility.BindableSource="{Binding Uri}" />
</UserControl>
